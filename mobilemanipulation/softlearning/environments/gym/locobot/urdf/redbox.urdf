    <visual>
      <origin rpy="0 0 0" xyz="0.0 0.0 0.0"/>
      <geometry>
        <box size="0.2 0.2 0.2"/>
      </geometry>
      <material name="red_mat">
        <color rgba="1.0 0.1 0.1 1"/>
      </material>
    </visual>
